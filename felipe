// Online C compiler to run C program online
#include <stdio.h>

int main() {
    int matheus;
    int jose;
    int soma;
    int multiplicacao;
    matheus = 24;
    jose = 23;
    soma = matheus + jose
    multiplicacao = matheus * jose 
    printf("o valor de matheus é %d \n", matheus);
    printf("o valor de jose é %d \n", jose);
    printf("o soma é %d \n" , soma);
    printf("a multiplicacao é %d", multiplicacao");
    return 0;
}
